#!/usr/bin/env python

import sys
import __builtin__

USE_CHECKOUT_SRC = False

if USE_CHECKOUT_SRC:
    sys.path.insert(0, '@jhbuilddir@')
    pkgdatadir = None
    datadir = None
else:
    pkgdatadir = "@pkgdatadir@"
    datadir = "@datadir@"

__builtin__.__dict__['PKGDATADIR'] = "@pkgdatadir@"
__builtin__.__dict__['DATADIR'] = "@datadir@"

import jhbuild.main
jhbuild.main.main(sys.argv[1:])
